{
  "name": "@react-comp/loadings",
  "version": "0.0.1",
  "description": "loadings",
  "main": "index.js",
  "keywords": [],
  "scripts": {
    "clean": "rimraf dist/",
    "dist": "npm run clean && webpack --config ./dev/webpack.config.js",
    "example": "webpack --config ./dev/webpack.example.js",
    "start": "node ./dev/serve.js",
    "prepublishOnly": "npm run dist && npm run example",
    "pub": "npm publish --access public",
    "test": "node ./dev/jest/start.js",
    "test:coverage": "npm run test -- --coverage",
    "ghpages": "npm run example && git subtree push --prefix example-dist/ origin gh-pages",
    "coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/react-comp/loadings"
  },
  "author": "moonlit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/react-comp/loadings/issues"
  },
  "files": [
    "dist",
    "index.js",
    "CHANGELOG.md"
  ],
  "homepage": "https://react-comp.github.io/loadings/",
  "devDependencies": {
    "@types/prop-types": "^15.5.8",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "autoprefixer": "^8.2.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "coveralls": "^3.0.2",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "node-sass": "^4.8.3",
    "postcss": "^6.0.21",
    "postcss-loader": "^2.1.3",
    "prop-types": "^15.6.1",
    "raf": "^3.4.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "ts-loader": "^5.3.0",
    "typescript": "^2.9.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.27.0",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.4",
    "webpack-serve": "^2.0.3"
  },
  "dependencies": {
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/dev/jest/setup.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/style-mock.js"
    },
    "testMatch": [
      "<rootDir>/**/__tests__/**/?(*.)(spec|test).{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ]
  }
}
